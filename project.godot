; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="La Tienda de Goblina"
config/description="En “La Tienda De Goblina” el jugador ayudará a Goblina a descubrir el encanto detrás de los artefactos que recolectará durante su aventura, y de esta manera, obtener la mejor venta posible. Descubre todos los artefactos y sus encantos para hacer de Goblina a la matriarca del clan Golagola."
run/main_scene="res://general_phase/start_screen.tscn"
config/features=PackedStringArray("4.0", "GL Compatibility")
config/icon="res://goblina_face.png"

[autoload]

TransitionManager="*res://general_phase/transition_manager.gd"
Database="*res://database/database.gd"
SoundtrackManager="*res://soundtrack/soundtrack_manager.tscn"

[display]

window/size/viewport_width=1400
window/size/viewport_height=1050
window/size/resizable=false

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray()

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
